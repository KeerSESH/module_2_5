def get_matrix (n, m, value):
    matrix = []
    for a in range(n):
        matrix.append([])
        for b in range(m):
            matrix[a].append(value)
    print(matrix)
    return matrix

n = int(input('Rows number:'))
m = int(input('Column number:'))
value = input('Matrix matter: ')
matrix = get_matrix(n, m, value)

for a in matrix:
    print(*a)
